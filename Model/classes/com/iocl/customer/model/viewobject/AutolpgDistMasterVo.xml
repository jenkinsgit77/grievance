  <SQLQuery><![CDATA[SELECT *                        
FROM                        
(SELECT                     
    A.DIST_NAME,                     
    A.UCM_UID,                     
    A.SAP_CODE,                     
    A.SOLD_TO_PARTY_CODE,                     
    A.MAIN_PH_NUM,                     
    A.MAIN_FAX_PH_NUM,                     
    A.MAIN_EMAIL_ADDR,                     
    A.CUST_SINCE_DT,                     
    A.ADDR ||A.ADDR_LINE_2||A.ADDR_LINE_3||A.ADDR_LINE_4 ADDR,                     
    A.STATE,                 
    G.VAL STATE_NAME,                
    A.PROVINCE,                     
    A.ZIPCODE,                     
    A.LONGITUDE,                     
    A.LATITUDE,                     
    A.SALES_AREA_CODE,                     
    A.SALES_AREA_NAME,                     
    A.AO_CODE,                     
    A.AO_NAME,                   
    rownum SNO,                       
ROUND((((acos(sin((((:LatitudeVar)*3.1415926)/180)) * sin((Latitude*3.1415926/180))+cos(((:LatitudeVar)*3.1415926/180)) * cos((Latitude*3.1415926/180)) * cos((((:LongitudeVar) - Longitude)*3.1415926/180))))*180/3.1415926)*60*1.1515*1.609344),10) AS distance                       
FROM  AUTOLPG_DIST_MASTER A,GRIEV_STATES G                         
WHERE A.STATE=G.CODE                        
) t                        
WHERE distance<5 order by distance asc]]></SQLQuery>
