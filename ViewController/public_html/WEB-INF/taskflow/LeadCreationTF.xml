<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="LeadCreationTF">
    <template-reference>
      <document>/WEB-INF/ExceptIonHandler/ExceptionHandlerTF.xml</document>
      <id>ExceptionHandlerTF</id>
    </template-reference>
    <default-activity>LeadCreation</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>LeadCreationBean</managed-bean-name>
      <managed-bean-class>com.iocl.lpg.customer.bean.LeadCreation</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <task-flow-call id="ConfirmationPageTF">
      <task-flow-reference>
        <document>/WEB-INF/taskflow/common/ConfirmationPageTF.xml</document>
        <id>ConfirmationPageTF</id>
      </task-flow-reference>
      <input-parameter id="__4">
        <name>confMessage</name>
        <value>#{pageFlowScope.confMessage}</value>
      </input-parameter>
      <input-parameter id="__5">
        <name>refNumber</name>
        <value>#{pageFlowScope.refNumber}</value>
      </input-parameter>
    </task-flow-call>
    <view id="LeadCreation">
      <page>/pageFragment/LeadCreation.jsff</page>
    </view>
    <control-flow-rule id="__2">
      <from-activity-id>LeadCreation</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>toConfirm</from-outcome>
        <to-activity-id>ConfirmationPageTF</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
